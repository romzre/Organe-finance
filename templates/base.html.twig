<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<nav>
			<div>
				<p>LOGO</p>
			</div>
			<div class="contMenu">
				<ul class="menu">
					<li>
						<a href="">
							Accueil
						</a>
					</li>
					<li>
						<a href="">
							FAQ
						</a>
					</li>
                    <li>
						<a href="">
							Tarifs
						</a>
					</li>
				</ul>
			</div>
			<div class="contLogin">

				<a href="{{ path("app_login") }}">
					<button class="btn btn-outline-primary">Connexion</button>
				</a>
				<a href="{{ path("app_registration") }}">
					<button class="btn btn-outline-secondary">Inscription</button>
				</a>
			</div>
		</nav>
		{% block body %}{% endblock %}
	</body>
</html>
